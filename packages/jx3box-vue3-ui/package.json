{
    "name": "@jx3box/jx3box-vue3-ui",
    "version": "0.9.10",
    "description": "JX3BOX Vue3 UI",
    "main": "index.js",
    "type": "module",
    "scripts": {
        "build": "vite build",
        "lint": "eslint .",
        "update": "npm --registry https://registry.npmjs.org install @jx3box/jx3box-common@latest @jx3box/jx3box-data@latest @jx3box/jx3box-emotion@latest @jx3box/jx3box-macro@latest @jx3box/jx3box-talent@latest",
        "header": "vue-cli-service build --target lib --name newheader src/Header.vue && cp public/index.html dist/newheader.html"
    },
    "eslintConfig": {
        "root": true,
        "env": {
            "node": true
        },
        "extends": [
            "plugin:vue/essential"
        ],
        "rules": {},
        "parserOptions": {
            "parser": "babel-eslint"
        }
    },
    "dependencies": {
        "@element-plus/icons-vue": "^2.1.0",
        "@jx3box/jx3box-common": "workspace:*",
        "@jx3box/jx3box-data": "workspace:*",
        "@jx3box/jx3box-emotion": "workspace:*",
        "@jx3box/jx3box-macro": "workspace:*",
        "@jx3box/jx3box-talent": "workspace:*",
        "@jx3box/reporter": "npm:@vl3box/reporter@0.1.4",
        "@tinymce/tinymce-vue": "^5.1.1",
        "@vueuse/core": "^9.13.0",
        "@vueuse/head": "^0.7.6",
        "axios": "^1.3.4",
        "core-js": "^3.8.3",
        "dayjs": "^1.11.7",
        "element-plus": "^2.2.36",
        "highlight.js": "^11.9.0",
        "install": "^0.13.0",
        "jquery": "^3.7.1",
        "js-base64": "^3.7.5",
        "katex": "^0.16.9",
        "lodash": "^4.17.21",
        "mitt": "^3.0.1",
        "qrcode.vue": "^3.3.4",
        "sortablejs": "^1.15.0",
        "url": "^0.11.3",
        "viewerjs": "^1.11.6",
        "vue": "^3.2.13",
        "vue-i18n": "^9.9.1",
        "vuedraggable": "^4.1.0"
    },
    "devDependencies": {
        "@babel/core": "^7.12.16",
        "@babel/eslint-parser": "^7.12.16",
        "cross-env": "^7.0.3",
        "csslab": "^4.0.3",
        "eslint": "^7.32.0",
        "eslint-plugin-vue": "^8.0.3",
        "husky": "^8.0.0",
        "less": "^4.1.3",
        "lint-staged": "^13.0.2",
        "prettier": "2.7.1"
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not dead",
        "not ie 11"
    ],
    "lint-staged": {
        "**/*": "prettier --write --ignore-unknown"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/JX3BOX/jx3box-vue3-ui.git"
    }
}
